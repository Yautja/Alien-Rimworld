<?xml version="1.0" encoding="utf-8" ?>

<Defs>
	<ThingDef ParentName="BaseFilth">
		<defName>Starbeast_Filth_AcidBlood</defName>
		<thingClass>Xenomorphtype.AcidBurnFilth</thingClass>
		<label>acidic fluid</label>
		<statBases>
			<Beauty>-30</Beauty>
			<Cleanliness>-10</Cleanliness>
		</statBases>
		<graphicData>
			<texPath>Things/Filth/Spatter</texPath>
			<color>(200, 220, 34, 180)</color>
		</graphicData>
		<tickerType>Normal</tickerType>
		<filth>
		  <ignoreFilthMultiplierStat>true</ignoreFilthMultiplierStat>
		  <disappearsInDays>2~4</disappearsInDays>
		  <rainWashes>false</rainWashes>
		  <cleaningWorkToReduceThickness>10</cleaningWorkToReduceThickness>
		  <canFilthAttach>false</canFilthAttach>
		  <cleaningSound>Interact_CleanFilth_Fluid</cleaningSound>
		</filth>
	</ThingDef>
	
