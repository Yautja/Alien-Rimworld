<?xml version="1.0" encoding="utf-8" ?>
<Patch>
<!-- Rimworld Specials -->
	<Operation Class="PatchOperationAddModExtension">
		<xpath>Defs/ThingDef[defName="Boomalope" or defName="Boomrat"]</xpath>
		<value>
			<li Class="Xenomorphtype.AnimalHostGenes">
				<genes>
					<li MayRequire="Ludeon.RimWorld.Biotech">Skin_PaleYellow</li>
					<li>XMT_Chemfuel_Metabolism</li>
				</genes>
			</li>
			<li Class="Xenomorphtype.AnimalMutateForms">
				<resultCandidates>
					<li>
						<thingTransformationTarget>XMT_Petrolsump</thingTransformationTarget>
						<pawnTransformationTarget></pawnTransformationTarget>
						<probability>1</probability>
						<essenceMinimum>-1</essenceMinimum>
						<essenceMaximum>-1</essenceMaximum>
					</li>
				</resultCandidates>
				<horrorPregnancy>XMT_BoomaHorrorPregnant</horrorPregnancy>
			</li>
		</value>
	</Operation>
	
